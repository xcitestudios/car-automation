{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "CommonJS",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2017",
    "lib": [ "ES2020", "DOM" ],
    "outDir": "./dist/lamba-src",
    "strict": true,
    "baseUrl": "./",
    "jsx": "react",
    "typeRoots": [ "node_modules/@types" ],
    "types": [ "node" ],
    "esModuleInterop": true
  },
  "include": [
    "lamba-src/**/*"
  ]
}
