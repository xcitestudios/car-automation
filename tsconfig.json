{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2016",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": false,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "sourceMap": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "compileOnSave": false,
  "exclude": [ "node_modules" ]
}