{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "CommonJS",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2017",
    "lib": [ "ES2020" ],
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./",
    "typeRoots": [ "node_modules/@types" ],
    "types": [ "node" ],
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}
